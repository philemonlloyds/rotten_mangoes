<h1>Welcome Admin</h1>
<table>
<colgroup span="3" class="columns"></colgroup>
<tr>
  <th>First Name</th>
  <th>Last Name</th>
  <th>Email</th>
  <th>Admin</th>
  <th>Delete</th>
</tr>
  <%@user.each do |user|%>
<tr>
  <td><%=user.firstname%></td>
  <td><%=user.lastname%></td>
  <td><%=user.email%></td>
  <td><%=user.admin%></td>
  <td><%=link_to "Delete_User",admin_user_path(user), method: :delete, confirm: "You sure?"%></td>
</tr>
<%end%>
</table>
